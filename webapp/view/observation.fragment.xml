<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="Observação">
		<content width="500px" height="250px">
			<TextArea  value="{Observacao}" showExceededText="true" maxLength="255" width="100%" height="250px" change="pegaObs"/>
			<Button text="Gravar" type="Accept" press="saveObservation"/>
		</content>
	</Dialog>
</core:FragmentDefinition>